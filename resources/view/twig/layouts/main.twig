{# main.twig file structure #}
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {{ style }}
    {% block style %}{% endblock %}

    {{ script }}
    {% block script %}{% endblock %}

    <title>{% block title %}{% endblock %}</title>
</head>
<body>
<div class="wrapper">
    <header></header>
    <main>
        <div class="container">
            {% block content %}{% endblock %}
        </div>
    </main>
    <footer></footer>
</div>
</body>

</html>
